version: "2"
services:
  web:
    container_name: tim-module
    build: .
    ports:
    - "80:3000"
    links:
    - mysql
  mysql:
    image: mysql:5.7
     volumes:
       - db_data:/var/lib/mysql
     restart: always
     environment:
       MYSQL_ROOT_PASSWORD: somewordpress
       MYSQL_DATABASE: wordpress
       MYSQL_USER: wordpress
       MYSQL_PASSWORD: wordpress